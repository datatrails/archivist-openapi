  "definitions": {
    "v1Asset": {
      "type": "object",
      "example": {
        "identity": "assets/add30235-1424-4fda-840a-d5ef82c4c96f",
        "type": "SimpleDeviceAsset",
        "display_name": "mydevice",
        "description": "my favourite asset",
        "confirmation_status": "CONFIRMATION_STATUS_CONFIRMED",
        "tracked": "TRACKED",
        "location_identity": "locations/11bf5b37-e0b8-42e0-8dcf-dc8c4aefc000",
        "extended_attributes": {
          "some": "properties"
        },
        "type_attributes": {
          "firmware_version": "v45.6",
          "serial_number": "f867662g.1"
        },
        "attachments": [
          {
            "attachment_identity": "attachments/11bf5b37-e0b8-42e0-8dcf-dc8c4aefc000",
            "display_name": "primary_image"
          },
          {
            "attachment_identity": "attachments/add30235-1424-4fda-840a-d5ef82c4c96f",
            "display_name": "form B7"
          }
        ]
      },
      "properties": {
        "identity": {
          "type": "string",
          "description": "relative resource address `assets/{UUID}`",
          "title": "assets/{UUID}",
          "readOnly": true,
          "maxLength": 4096
        },
        "type": {
          "type": "string",
          "description": "pre-defined asset type",
          "readOnly": true
        },
        "display_name": {
          "type": "string",
          "description": "user defined display name",
          "readOnly": true,
          "maxLength": 128
        },
        "location_identity": {
          "type": "string",
          "description": "identity of a location resource `locations/11bf5b37-e0b8-42e0-8dcf-dc8c4aefc000`",
          "readOnly": true,
          "maxLength": 4096
        },
        "tracked": {
          "$ref": "#/definitions/v1AssetTrackedStatus",
          "description": "indicates whether asset is still being tracked in the system",
          "readOnly": true
        },
        "extended_attributes": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "title": "user defined attributes key max_length: 1024 value max_length: 4096"
        },
        "confirmation_status": {
          "$ref": "#/definitions/v1ConfirmationStatus",
          "description": "indicates if the asset has been succesfully committed to the blockchain",
          "readOnly": true
        },
        "attachments": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/v1AttachmentInfo"
          },
          "description": "set of attachments associated with this Asset",
          "readOnly": true
        },
        "type_attributes": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "title": "type specific attributes key max_length: 1024 value max_length: 4096"
        },
        "description": {
          "type": "string",
          "description": "short description of an asset",
          "readOnly": true,
          "maxLength": 4096
        }
      },
      "description": "This represents the asset and its properties, `asset_identity` is UUID assigned to that asset, `type_attributes` is the asset specific data that should be only visible on specific asset detail page"
    },
    "v1AssetTrackedStatus": {
      "type": "string",
      "enum": [
        "UNSPECIFIED",
        "TRACKED",
        "NOT_TRACKED"
      ],
      "default": "UNSPECIFIED"
    },
    "v1AttachmentInfo": {
      "type": "object",
      "example": {
        "identity": "attachments/11bf5b37-e0b8-42e0-8dcf-dc8c4aefc000",
        "label": "form B5",
        "hash": "0x0572678487FF"
      },
      "properties": {
        "attachment_identity": {
          "type": "string",
          "description": "identity of a attachment resource `attachments/11bf5b37-e0b8-42e0-8dcf-dc8c4aefc000`",
          "title": "attachments/{UUID}",
          "maxLength": 4096
        },
        "display_name": {
          "type": "string",
          "description": "user defined display_name for attachment",
          "maxLength": 128
