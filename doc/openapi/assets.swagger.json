    "v1ConfirmationStatus": {
      "type": "string",
      "enum": [
        "CONFIRMATION_STATUS_UNSPECIFIED",
        "CONFIRMATION_STATUS_PENDING",
        "CONFIRMATION_STATUS_CONFIRMED",
        "CONFIRMATION_STATUS_FAILED"
      ],
      "default": "CONFIRMATION_STATUS_UNSPECIFIED"
